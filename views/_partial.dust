<script type="text/html" id="wwm-svg-logo">
	<svg class="svg-logo" viewbox="0 0 168 300">
		<filter id="dropshadow" height="130%">
			<feGaussianBlur in="SourceAlpha" stdDeviation="8"/> <!-- stdDeviation is how much to blur -->
			<feOffset dx="5" dy="7" result="offsetblur"/> <!-- how much to offset -->
			<feComposite in2="offsetblur" operator="in"/>
			<feMerge>
				<feMergeNode/> <!-- this contains the offset blurred image -->
				<feMergeNode in="SourceGraphic"/> <!-- this contains the element that the filter is applied to -->
			</feMerge>
		</filter>
		<polygon class="triangle triangle-0" points="5,100 105,100 55,13.4"/>
		<polygon class="triangle triangle-1" points="0,110 100,110 50,196.6"/>
		<polygon class="triangle triangle-2" points="60,200 160,200 110,113.4"/>
		<polygon class="triangle triangle-3" points="55,210 155,210 105,296.6"/>
	</svg>
</script>
<script type="text/html" id="wwm-canvas-logo">
	<canvas class="canvas-logo" width="150" height="150"></canvas>
</script>
<script type="text/html" id="wwm-login">
	<div id="login-logo"></div>
	<div id="login-wrapper">
		<div>
			<img id="kakao-login-btn" src="/kakao_account_login_btn_medium_narrow.png"/>
		</div>
		<div>
			<img id="fb-login-btn" src="/facebook.png"/>
		</div>
		<div id="localhost-login">로컬호스트 로그인</div>
		<div id="localhost2-login">로컬호스트2 로그인</div>
	</div>
</script>
<script type="text/html" id="wwm-lobby">
	<header id="lobby-header">
		<div id="lobby-logo"></div>
		<div id="profile">
			<img id="profile-picture" width="30" height="30" src="{~lb}picture{~rb}"/>
			<span id="profile-name">{~lb}name{~rb}</span>
		</div>
		<div id="profile-menu">
			<button id="refresh-profile"><i class="fa fa-refresh"></i></button>
			<button id="logout-btn">로그아웃</button>
		</div>
	</header>
	<main id="lobby-main">
		<div id="room-list">
			<div class="title sr-only">리스트</div>
			<div id="rooms"></div>
			<button id="refresh-list">
				<i class="fa fa-refresh"><span class="sr-only">새로고침</span></i>
			</button>
			<button id="show-createroom-modal"><i class="fa fa-plus"></i><span class="sr-only">방 만들기</span></button>
		</div>
	</main>
	<footer id="lobby-footer">
		<div id="search">
			<form class="input-group">
				<input type="text" placeholder="방 검색"/>
					<span class="input-group-btn">
						<button id="searchroom-btn">
							<i class="fa fa-search"><span class="sr-only">검색</span></i>
						</button>
					</span>
			</form>
		</div>
	</footer>
</script>
<script type="text/html" id="wwm-create-modal">
	<header id="create-header"><h1>방생성</h1></header>
	<form>
		<main id="create-main">
			<div class="form-group">
				<label for="room-title">방제목</label>
				<input id="room-title"/>
			</div>
			<div class="form-group">
				<label for="room-limit">인원수</label>
				<select id="room-limit">
					<option value="2">2</option>
					<option value="3">3</option>
					<option value="4">4</option>
					<option value="5">5</option>
					<option value="6">6</option>
					<option value="7">7</option>
					<option value="8">8</option>
				</select>
			</div>
			<div class="form-group">
				<label for="room-password">비밀번호</label>
				<input id="room-password" placeholder="빈칸이면 공개방">
			</div>
		</main>
		<footer id="create-footer">
			<button id="create-room-btn">방생성</button>
			<button class="modal-close">취소</button>
		</footer>
	</form>
</script>
