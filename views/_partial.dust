<script type="text/html" id="wwm-lobby">
	<header id="lobby-header">
		<div id="lobby-logo"></div>
		<div id="profile">
			<img id="profile-picture" width="30" height="30" src="{~lb}picture{~rb}"/>
			<span id="profile-name">{~lb}name{~rb}</span>
		</div>
		<div id="profile-menu">
			<button id="refresh-profile"><i class="fa fa-refresh"></i></button>
			<button id="logout-btn">로그아웃</button>
		</div>
	</header>
	<main id="lobby-main">
		<div id="room-list">
			<div class="title sr-only">리스트</div>
			<div id="rooms"></div>
			<button id="refresh-list">
				<i class="fa fa-refresh"><span class="sr-only">새로고침</span></i>
			</button>
			<button id="show-createroom-modal"><i class="fa fa-plus"></i><span class="sr-only">방 만들기</span></button>
		</div>
	</main>
	<footer id="lobby-footer">

		<div id="search">
			<form class="input-group">
				<input type="text" placeholder="방 제목"/>
					<span class="input-group-btn">
						<button id="searchroom-btn">
							<i class="fa fa-search"><span class="sr-only">검색</span></i>
						</button>
					</span>
			</form>
		</div>
	</footer>
</script>
<script type="text/html" id="wwm-createroom-modal">
	<header id="createroom-header"><h1>방생성</h1></header>
	<form>
		<main id="createroom-main">
			<div class="form-group">
				<label for="room-title">방제목</label>
				<input id="room-title"/>
			</div>
			<div class="form-group">
				<label for="room-people-limit">인원수</label>
				<select id="room-people-limit">
					<option value="2">2</option>
					<option value="3">3</option>
					<option value="4">4</option>
					<option value="5">5</option>
					<option value="6">6</option>
					<option value="7">7</option>
					<option value="8">8</option>
				</select>
			</div>
			<div class="form-group">
				<label for="room-password">비밀번호</label>
				<input id="room-password" placeholder="빈칸이면 공개방">
			</div>
		</main>
		<footer id="createroom-footer">
			<button id="create-room-btn">방생성</button>
			<button class="modal-close">취소</button>
		</footer>
	</form>
</script>
