<script type="text/html" id="wwm-svg-logo">
	<svg class="svg-logo" viewbox="0 0 168 300">
		<filter id="dropshadow" height="130%">
			<feGaussianBlur in="SourceAlpha" stdDeviation="2"/> <!-- stdDeviation is how much to blur -->
			<feOffset dx="5" dy="7" result="offsetblur"/> <!-- how much to offset -->
			<feComposite in2="offsetblur" operator="in"/>
			<feMerge>
				<feMergeNode/> <!-- this contains the offset blurred image -->
				<feMergeNode in="SourceGraphic"/> <!-- this contains the element that the filter is applied to -->
			</feMerge>
		</filter>
		<polygon class="triangle triangle-0" points="5,100 105,100 55,13.4"/>
		<polygon class="triangle triangle-1" points="0,110 100,110 50,196.6"/>
		<polygon class="triangle triangle-2" points="60,200 160,200 110,113.4"/>
		<polygon class="triangle triangle-3" points="55,210 155,210 105,296.6"/>
	</svg>
</script>
<script type="text/html" id="wwm-canvas-logo">
	<canvas class="canvas-logo" width="150" height="150"></canvas>
</script>
<script type="text/html" id="wwm-login">
	<div id="login-logo"></div>
	<div id="login-wrapper">
		<div>
			<img id="kakao-login-btn" src="/kakao_account_login_btn_medium_narrow.png"/>
		</div>
		<div>
			<img id="fb-login-btn" src="/facebook.png"/>
		</div>
